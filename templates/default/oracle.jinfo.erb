name=<%= @name %>
<% unless @alias_name == @name %>
alias=<%= @alias_name %>
<% end %>
priority=<%= @priority %>
section=main

<% @jre_cmds.each do |cmd| %>
jre <%= cmd %> <%= @app_home %>/jre/bin/<%= cmd %>
<% end %>
<% @jdk_cmds.each do |cmd| %>
jdk <%= cmd %> <%= @app_home %>/bin/<%= cmd %>
<% end %>
